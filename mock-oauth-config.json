{
  "interactiveLogin": true,
  "httpServer": {
    "port": 8080,
    "hostname": "0.0.0.0"
  },
  "tokenCallbacks": [
    {
      "issuerId": "default",
      "tokenExpiry": 3600,
      "requestMappings": [
        {
          "requestParam": "scope",
          "match": "*",
          "claims": {
            "sub": "#optional:sub",
            "name": "#optional:name",
            "email": "#optional:email",
            "picture": "#optional:picture",
            "preferred_username": "#optional:preferred_username"
          }
        }
      ]
    }
  ]
}
